{
	"name": "nodetsscaffold",
	"version": "1.0.0",
	"description": "",
	"main": "build/index.js",
	"types": "build/index.d.ts",
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"build": "rimraf ./build && tsc --sourceMap false --removeComments true",
		"start": "npm run build && node build/index.js",
		"debug": "NODE_ENV=development nodemon -e ts,js,json --exec \"node --inspect=0.0.0.0:9229 --require ts-node/register src/index.ts\"",
		"docker:image": "docker build . -t scaffold/web-app",
		"docker:create": "docker create --name scaffold-web-app-container -p 8080:3000 scaffold/web-app",
		"docker:remove": "docker stop scaffold-web-app-container && docker rm scaffold-web-app-container && docker rmi scaffold/web-app",
		"docker:start": "docker start scaffold-web-app-container",
		"build:prod": "yarn run build && yarn run docker:image && yarn run docker:create",
		"docker:dev": ""
	},
	"author": "",
